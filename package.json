{
  "name": "javascript-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel serve lint:watch test:watch",
    "serve": "nodemon --exec npm run serve:babel-node -- ./bin/www",
    "serve:babel-node": "babel-node",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel start localtunnel",
    "lint": "esw webpack.config.dev public/javascripts routes bin/www",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress test.js \"public/javascripts/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "rimraf ./dist&& mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node build.js"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.16.3",
    "body-parser": "~1.18.2",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "debug": "~3.1.0",
    "eslint": "^4.17.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-watch": "^3.1.3",
    "express": "~4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "hbs": "~4.0.1",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "11.6.2",
    "localtunnel": "^1.8.1",
    "mocha": "^5.0.0",
    "morgan": "~1.9.0",
    "nock": "^9.1.6",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^1.14.12",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "open": "^0.0.5",
    "rimraf": "^2.5.4",
    "serve-favicon": "~2.4.2",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-md5-hash": "^0.0.6"
  }
}
